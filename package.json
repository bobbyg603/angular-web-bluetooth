{
  "name": "angular-web-bluetooth-starter",
  "version": "11.2.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prestart": "npm run build",
    "prebuild": "ng build @manekinekko/angular-web-bluetooth --prod && cp README.md dist/manekinekko/angular-web-bluetooth/",
    "build": "ng build angular-web-bluetooth-starter --prod",
    "postbuild": "npm run docs",
    "test": "ng test",
    "test:lib": "ng test @manekinekko/angular-web-bluetooth",
    "test:starter": "ng test angular-web-bluetooth-starter",
    "lint": "ng lint",
    "e2e": "echo 'skipping test'",
    "docs": "compodoc -p tsconfig.json --theme material -d docs",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prerelease": "npm run prebuild",
    "release": "cd dist/manekinekko/angular-web-bluetooth && release-it --no-git.requireUpstream"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.17",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "~12.2.17",
    "@angular/compiler": "~12.2.17",
    "@angular/core": "~12.2.17",
    "@angular/forms": "~12.2.17",
    "@angular/material": "^11.2.0",
    "@angular/platform-browser": "~12.2.17",
    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular/router": "~12.2.17",
    "@release-it/conventional-changelog": "^1.1.4",
    "@types/web-bluetooth": "0.0.6",
    "rxjs": "~6.6.3",
    "smoothie": "^1.35.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.1.0",
    "@angular-devkit/build-angular": "^0.1102.0",
    "@angular-devkit/core": "^11.2.0",
    "@angular/cli": "~11.2.0",
    "@angular/compiler-cli": "~12.2.17",
    "@angular/language-service": "~12.2.17",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.0.13",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.3",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "release-it": "^13.6.3",
    "ts-node": "~8.10.2",
    "tsickle": "^0.38.1",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}